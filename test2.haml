	%body

		#loading-wrap{:style=>'dislay:none'}
			.loading-effect

		%header.Sticky_Header.sticky-top
			%nav.navbar.navbar-expand-lg.navbar-light.pt-4.pb-4
				.container
					%a.navbar-brand{ :href => "index.html"}
						%img{:alt=>"Logo", :src=>"logo.png"}
					%button{:class=>'navbar-toggler', :type=>'button', :"data-toggle"=>'collapse', :"data-target"=>'#menu', :"aria-controls"=>'menu', :"aria-expanded"=>'true', :"aria-label"=>'Toggle Navigation'}
						%span.navbar-toggler-icon
					.collapse.navbar-collapse#menu
						%ul{:class=>"nav navbar-nav navigation ml-auto clearfix"}
							%li.nav-item.active
								%a{ :class=>"nav-link", :href=>"index.html", :title=>"Home"}Home
								%span.sr-only
							%li.nav-item
								%a{ :class=>"nav-link",:href=>"#", :title=>"About Us"}About Us
							%li.nav-item
								%a{ :class=>"nav-link", :href=>"#", :title=>"Portfolio"}Portfolio
							%li.nav-item
								%a{ :class=>"nav-link", :href=>"#", :title=>"Blog"}Blog
							%li.nav-item
								%a{ :class=>"nav-link", :href=>"#", :title=>"Pages"}Pages
							%li.nav-item
								%a{ :class=>"nav-link", :href=>"#", :title=>"Contact"}Contact Us
						.g-signin2{ :"data-onsuccess"=>"onSignIn", :"data-theme"=>"dark"}
						.info
							%button#info{ :type=>"button", :class=>"btn btn-success", :onclick=>"signOut()", :style=>"display:none;"}Info
						.test
							%button#test{ :type=>"button", :class=>"btn", :onclick=>"signOut()", :style=>"display:none;"}Test
						.sign-out
							%button#out{ :type=>"button", :class=>"btn btn-danger", :onclick=>"signOut()", :style=>"display:none;"}Sign-Out
						
